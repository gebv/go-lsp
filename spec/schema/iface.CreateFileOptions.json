{
    "$id": "/CreateFileOptions",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Options to create a file.",
    "properties": {
        "ignoreIfExists": {
            "description": "Ignore if exists.",
            "type": "boolean"
        },
        "overwrite": {
            "description": "Overwrite existing file. Overwrite wins over `ignoreIfExists`",
            "type": "boolean"
        }
    },
    "type": "object"
}

